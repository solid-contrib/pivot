@prefix : <#>.
@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

:ControlReadWrite
    a acl:Authorization;
    acl:accessTo <./>;
    acl:agent <{{webId}}>;
    acl:default <./>;
    acl:mode acl:Control, acl:Read, acl:Write.
:Read
    a acl:Authorization;
    acl:accessTo <./>;
    acl:agentClass foaf:Agent;
    acl:default <./>;
    acl:mode acl:Read.
